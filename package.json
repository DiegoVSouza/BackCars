{
  "name": "RENTALX",
  "version": "1.0.0",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": " jest --runInBand --detectOpenHandles",
    "migrate": "prisma migrate dev --schema ./src/shared/infra/prisma/schema.prisma",
    "push": "prisma db push --schema ./src/shared/infra/prisma/schema.prisma",
    "studio": "prisma studio --schema ./src/shared/infra/prisma/schema.prisma",
    "dev": "yarn prisma generate && ts-node-dev --respawn --transpile-only src/shared/infra/http/server.ts --schema ./src/shared/infra/prisma/schema.prisma ",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli",
    "migration:create": "cd src/shared/infra/typeorm/migrations && npx typeorm-ts-node-commonjs migration:create",
    "migration:generate": "cd src/shared/infra/typeorm/migrations && npx typeorm-ts-node-commonjs migration:generate -d ../dataSourceConfig.ts",
    "migration:revert": "cd src/shared/infra/typeorm/migrations && npx typeorm-ts-node-commonjs migration:revert -d ../dataSourceConfig.ts",
    "migration:up": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d src/database",
    "migration:run": "cd src/shared/infra/typeorm/migrations && npx typeorm-ts-node-commonjs migration:run -d ../dataSourceConfig.ts",
    "schema:drop": "cd src/shared/infra/typeorm/migrations && npx typeorm-ts-node-commonjs schema:drop -d ../dataSourceConfig.ts",
    "seed:admin": "ts-node-dev src/shared/infra/typeorm/seed/admin.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "csv-parse": "^5.0.4",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.1",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "pg": "^8.7.3",
    "prisma": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.2.4",
    "swagger-ui-express": "^4.3.0",
    "ts-node": "10.9.1",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.7",
    "unit-tests": "^1.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/csv-parse": "^1.2.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.15",
    "ts-jest": "^28.0.7",
    "ts-node-dev": "2.0.0",
    "typescript": "^4.6.3"
  }
}